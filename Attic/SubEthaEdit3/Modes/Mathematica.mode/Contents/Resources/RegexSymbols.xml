<symbols>
	<!-- Find Mathematica function definitions with an included "=" sign. -->
	<symbol id="Matlab Functions with Assignment" font-weight="normal" font-style="normal" image="SymbolF" indentation="0" ignoreblocks="yes">
		<regex>(([a-zA-Z0-9$]+\[(\s*|([a-zA-Z0-9$]*[^\[\]]*))\])|([a-zA-Z0-9$]+::usage)|BeginPackage\[(\w)*\]|EndPackage\[\]|Begin\["`Private`"\])</regex>
    </symbol>
</symbols>